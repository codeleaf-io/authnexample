[
    {
        "id": "basic",
        "type": "authenticator",
        "implementation": "io.codeleaf.authn.jaxrs.basic.BasicJaxrsAuthenticator",
        "configuration": {
            "passwordRequestAuthenticator": "custom"
        }
    },
    {
        "id": "custom",
        "type": "authenticator",
        "implementation": "io.codeleaf.authn.password.custom.CustomPasswordRequestAuthenticator",
        "configuration": {
            "databasePassword": "xxx"
        }
    },
    {
        "id": "normalEndpoints",
        "type": "endpoints",
        "protocol": "jaxrs",
        "endpoints": [ "/authn", "/authn/*", "/info" ],
        "authenticator": "basic",
        "authentication-policy": "optional"
    },
    {
        "id": "adminEndpoints",
        "type": "endpoints",
        "protocol": "jaxrs",
        "endpoints": [ "/admin/*" ],
        "authenticator": "basic",
        "authentication-policy": "required"
    },
    {
        "id": "adminSecretEndpoints",
        "type": "endpoints",
        "protocol": "jaxrs",
        "endpoints": [ "/admin/secret/*" ],
        "authenticator": "basic-dummy",
        "authentication-policy": "required"
    },
    {
        "id": "basic-dummy",
        "type": "authenticator",
        "implementation": "io.codeleaf.authn.jaxrs.basic.BasicJaxrsAuthenticator",
        "configuration": {
            "passwordRequestAuthenticator": "dummy"
        }
    },
    {
        "id": "dummy",
        "type": "authenticator",
        "implementation": "io.codeleaf.authn.password.dummy.DummyPasswordRequestAuthenticator",
        "configuration": {
            "userName": "admin",
            "password": "12345"
        }
    }
]
