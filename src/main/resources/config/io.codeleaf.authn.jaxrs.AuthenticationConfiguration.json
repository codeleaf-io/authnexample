{
  "authenticators": {
    "dummy": {
      "implementation": "io.codeleaf.authn.password.dummy.DummyAuthenticator",
      "configuration": {
        "type": "io.codeleaf.authn.password.dummy.DummyConfiguration",
        "settings": {
          "userName": "admin",
          "password": "12345"
        }
      }
    },
    "basic": {
      "implementation": "io.codeleaf.authn.jaxrs.basic.BasicAuthenticator",
      "configuration": {
        "type": "io.codeleaf.authn.jaxrs.basic.BasicConfiguration",
        "settings": {
          "passwordAuthenticator": "dummy"
        }
      }
    },
    "linkedin": {
      "implementation": "io.codeleaf.authn.jaxrs.oauth.linkedin.LinkedInAuthenticator",
      "configuration": {
        "type": "io.codeleaf.authn.jaxrs.oauth.OAuthConfiguration",
        "settings": {
          "clientId": "<registered clientId>",
          "clientSecret": "<registered client secret>",
          "redirectUri": "<redirection url or landing page after login>",
          "scope": "r_basicprofile r_emailaddress",
          "state": "some_params"
        }
      }
    }
  },
  "zones": {
    "admin": {
      "endpoints": [
        "admin/*",
        "admin"
      ],
      "authenticator": "linkedin",
      "policy": "redirect"
    },
    "public": {
      "endpoints": [
        "public/*",
        "public"
      ],
      "policy": "none"
    },
    "default": {
      "endpoints": [
        "hello"
      ],
      "authenticator": "basic",
      "policy": "optional"
    }
  }
}